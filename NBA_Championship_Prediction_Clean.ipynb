      "source": [
        "# ê¸°ë³¸ í”¼ì²˜ ì •ì˜\n",
        "base_features = [\n",
        "    'FGM',           # ì„±ê³µí•œ ìŠ› ìˆ˜\n",
        "    'PLUS_MINUS_x',  # íŒ€ ë“ì‹¤ì°¨\n",
        "    'FG_PCT_x',      # íŒ€ ìŠ› ì„±ê³µë¥ \n",
        "    'AST_x',         # íŒ€ ì–´ì‹œìŠ¤íŠ¸\n",
        "    'FG3M',          # ì„±ê³µí•œ 3ì ìŠ› ìˆ˜\n",
        "    'DREB',          # ìˆ˜ë¹„ ë¦¬ë°”ìš´ë“œ\n",
